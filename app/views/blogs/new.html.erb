
<h1>New Blog Entry</h1>

<%= form_for :blog, url: blogs_path do |f| %>
  <p>
    <%= f.label :blog_name %><br>
    <%= f.text_field :blog_name %>
  </p>

  <p>
    <%= f.label :blog_content %><br>
    <%= f.text_area :blog_content %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', blogs_path %>
